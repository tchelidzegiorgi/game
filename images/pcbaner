 $(document).ready(function(){
      $('.category-items ul').bxSlider({
      	minSlides: 6,
        maxSlides: 6,
        slideMargin: 10,
        hideControlOnEnd: true,
        touchEnabled: false,
        infiniteLoop: false,
        slideWidth: ($(window).width()),
        controls: 1,
        nextText:'',
        prevText:'',
        pager: 0,
        onSliderLoad: function(){
        $(".category-items").css("visibility", "visible");
      }
      	});

      $('.ty-btn__secondary.ty-btn').on('click', function(){
        $('label').removeClass('cm-required');
      });
      $(".bx-controls-direction .bx-prev").addClass("uni-left-sight");
        $(".bx-controls-direction .bx-next").addClass("uni-right-sight");
    });

 if ( $(window).width() < 430) {
  $('.category-items ul').bxSlider({
        minSlides: 2,
        maxSlides: 2,
        slideMargin: 10,
        hideControlOnEnd: true,
        infiniteLoop: false,
        slideWidth: ($(window).width()),
        controls: 1,
        nextText:'',
        prevText:'',
        pager: 0,
        onSliderLoad: function(){
        $(".category-items").css("visibility", "visible");
      }
        });
}





