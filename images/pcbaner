(function(_, $) {
    $.ceEvent('on', 'ce.commoninit', function(context) {
        var slider = context.find('#banner_slider_523');
        if (slider.length) {
            slider.owlCarousel({
                direction: 'ltr',
                items: 1,
                singleItem : true,
                slideSpeed: 400,
                autoPlay: '3000',
                stopOnHover: true,
                                                                                    pagination: false,
                    navigation: true,
                    navigationText: ['Prev', 'Next']
                            });
        }
    });
}(Tygh, Tygh.$));
